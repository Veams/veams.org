        "@veams/plugin-dom": "^1.0.0",
        "@veams/plugin-logger": "^1.0.0",
        "@veams/plugin-media-query-handler": "^1.1.0",
        "@veams/plugin-mixins": "^1.0.0",
        "@veams/plugin-modules": "^1.2.0",
        "@veams/plugin-store": "^0.0.2",
        "@veams/plugin-templater": "^1.0.0",
        "@veams/plugin-vent": "^1.0.0",
        "@veams/query": "^2.4.2",
        "handlebars": "^4.0.12",
        "lazysizes": "^4.1.2",
        "mangony-hbs-helpers": "^0.6.3"
    },
