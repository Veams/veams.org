{
    "name": "veams",
    "description": "Custom project description",
    "version": "0.0.1",
    "engines": {
        "node": ">=8.0.0"
    },
    "scripts": {
        "clear:app": "rimraf app/**",
        "local:server": "cross-env BABEL_ENV=server nodemon src/server/index.js --config ./configs/tasks/nodemon.config.json",
        "prestart": "npm run clear:app",
        "start": "cross-env NODE_ENV=local npm-run-all --parallel local:*",
        "dev:build": "cross-env NODE_ENV=dev npm run build",
        "qa:build": "cross-env NODE_ENV=qa npm run build",
        "prod:build": "meta git update && meta git checkout master && cross-env NODE_ENV=production npm run build",
        "local:start": "webpack --watch --mode=development --hide-modules",
        "build": "npm run clear:app && cross-env BABEL_ENV=client webpack && npm run html:generate",
        "sprite:generate": "node configs/tasks/icons/sprite.js",
        "images:generate": "node configs/tasks/images/images-resizer.js",
        "images:export": "node configs/tasks/images/images-export.js",
        "html:generate": "node configs/tasks/mangony/mangony-build.js"
    },
    "dependencies": {
        "@veams/base": "^1.0.0",
        "@veams/component": "^1.2.5",
        "@veams/core": "^1.0.5",
        "@veams/helpers": "^1.3.1",
